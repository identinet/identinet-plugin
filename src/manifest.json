{
  "manifest_version": 3,
  "name": "identinet",
  "description": "Displays and verifies decentralized identity information of websites",
  "version": "0.1.0",
  "homepage_url": "https://github.com/identinet/identinet-plugin",
  "icons":
  {
    "16": "icons/favicon_16.png",
    "32": "icons/favicon_32.png",
    "48": "icons/favicon_48.png",
    "96": "icons/favicon_96.png",
    "128": "icons/favicon_128.png"
  },
  "action":
  {
    "default_title": "Hello my friend",
    "default_popup": "hello.html",
    "default_icon": "icons/favicon_48.png"
  },
  "permissions":
  [
    "activeTab"
  ],
  "background":
  {
    "scripts":
    [
      "background.js"
    ]
  },
  "browser_specific_settings":
  {
    "gecko":
    {
      "id": "extension@identinet.io",
      "strict_min_version": "109.0"
    }
  }
}